<MudTextField
    T="string"
    Validation="@ValidationAttribute"
    HelperText="@HelperText"
    Required="@IsRequired"
    Variant="@Variant"
    Immediate="@Immediate"
    RequiredError="@RequiredError"
    InputType="@InputType"
    Label="@Label"
    @bind-value="@Value"
    @oninput="@OnValueChanged">

</MudTextField>



